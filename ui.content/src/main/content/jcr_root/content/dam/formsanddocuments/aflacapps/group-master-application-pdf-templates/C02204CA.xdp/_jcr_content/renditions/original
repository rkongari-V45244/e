         <subform layout="tb" name="HI1" w="576pt">
            <variables>
               <script contentType="application/x-javascript" name="scriptHI">
function hideseg(){
if(HI1.GroupHospitalIndemnity.GA.TextField24.rawValue=="Group Hospital Indemnity"){
HI1.GroupHospitalIndemnity.GA.TextField24.rawValue=HI1.GroupHospitalIndemnity.GA.TextField24.rawValue.toUpperCase();
}
else{
HI1.presence="hidden";
}
}

function hide(){
if(HI1.GroupHospitalIndemnityPremiumContribution.GroupPolicy.CheckBox15.rawValue=="No") {
HI1.GroupHospitalIndemnityPremiumContribution.TextField19.presence="hidden";
HI1.GroupHospitalIndemnityPremiumContribution.TextField20.presence="hidden";
HI1.GroupHospitalIndemnityPremiumContribution.GroupPolicy.CheckBox14.presence="hidden";
}
else{
HI1.GroupHospitalIndemnityPremiumContribution.GroupPolicy.CheckBox15.presence="hidden";
}
}

function hideCB(){
tl=xfa.resolveNodes("HI1.GroupHospitalIndemnityOptionalFeatures.Table1.Row1.productCoverages.productCoverage[*]").length;
for(i=0;i&lt;tl;i++){
if(xfa.resolveNode("HI1.GroupHospitalIndemnityOptionalFeatures.Table1.Row1.productCoverages.productCoverage["+(i)+"].coveragelevel.productCoverageLevel").rawValue==null){
xfa.resolveNode("HI1.GroupHospitalIndemnityOptionalFeatures.Table1.Row1.productCoverages.productCoverage["+(i)+"].coveragelevel.CheckBox20").presence="hidden";
}
}
}

function hideothers(){
if(HI1.GroupHospitalIndemnityEligibleEmployees.Eligible3.TextField16.rawValue==null){
HI1.GroupHospitalIndemnityEligibleEmployees.Eligible3.presence="hidden";
}
}

function hideexcept(){
if(HI1.GroupHospitalIndemnityEligibleEmployees.TextField15.rawValue==null){
HI1.GroupHospitalIndemnityEligibleEmployees.TextField15.presence="hidden";
}
}

function accidentreason(){
if(HI1.GroupHospitalIndemnity.ApplicationReason.CheckBox1.rawValue=="New Application"){
HI1.GroupHospitalIndemnity.ApplicationReason.CheckBox3.presence="hidden";
HI1.GroupHospitalIndemnity.ApplicationReason.TextField13.presence="hidden";
HI1.GroupHospitalIndemnity.Other.presence="hidden";
}
else if(HI1.GroupHospitalIndemnity.ApplicationReason.CheckBox3.rawValue=="Change to Existing Aflac Policy"){
HI1.GroupHospitalIndemnity.ApplicationReason.CheckBox1.presence="hidden";
HI1.GroupHospitalIndemnity.Other.presence="hidden";
}
else if(HI1.GroupHospitalIndemnity.Other.CheckBox4.rawValue=="Other"){
HI1.GroupHospitalIndemnity.ApplicationReason.presence="hidden";
}
}

function age(){
if(HI1.GroupHospitalIndemnityEligibleEmployees.Eligible2.TextField2.rawValue=="Between Age"){
xfa.resolveNode("HI1.GroupHospitalIndemnityEligibleEmployees.Eligible2.TextField3").rawValue=xfa.resolveNode("HI1.GroupHospitalIndemnityEligibleEmployees.Eligible2.TextField3").rawValue+" and ";
xfa.resolveNode("HI1.GroupHospitalIndemnityEligibleEmployees.Eligible2.TextField5").rawValue="";
}
}

function hidespouse(){
if(HI1.GroupHospitalIndemnityEligibleEmployees.Eligible2.TextField21.rawValue=="Yes"){
HI1.GroupHospitalIndemnityEligibleEmployees.Eligible2.TextField21.presence="hidden"
}
else{
HI1.GroupHospitalIndemnityEligibleEmployees.Eligible2.presence="hidden"
}
}

function ageemp(){
if(HI1.GroupHospitalIndemnityEligibleEmployees.Eligible1.TextField2.rawValue=="Between Age"){
xfa.resolveNode("HI1.GroupHospitalIndemnityEligibleEmployees.Eligible1.TextField3").rawValue=xfa.resolveNode("HI1.GroupHospitalIndemnityEligibleEmployees.Eligible1.TextField3").rawValue+" and ";
xfa.resolveNode("HI1.GroupHospitalIndemnityEligibleEmployees.Eligible1.TextField5").rawValue="";
}
}

function hidepif(){
if(HI1.GroupHospitalIndemnityPremiumContribution.minimumNum.TextField34.rawValue=="No"){
HI1.GroupHospitalIndemnityPremiumContribution.minimumNum.presence="hidden";
}
else{
HI1.GroupHospitalIndemnityPremiumContribution.minimumNum.TextField34.presence="hidden";
}
}
