{
    "path": [ "/content"],
    "idpCertAlias": "$[env:SAML_IDP_CERT_ALIAS;default=certalias___1652125559800]",
    "idpIdentifier": "$[env:SAML_IDP_ID;default=AflacDev]",
    "idpUrl": "$[env:SAML_IDP_URL;default=https://fimd.nt.lab.com:9031/idp/SSO.saml2]",
    "serviceProviderEntityId": "$[env:SAML_AEM_ID;default=https://publish-p123-e456.adobeaemcloud.com]",
    "defaultRedirectUrl": "$[env:SAML_AEM_ID;default=https://publish-p123-e456.adobeaemcloud.com]",
    "userIDAttribute":"UserId",
    "useEncryption": false,
    "createUser": true,
    "userIntermediatePath": "aflacapps/ping",
    "synchronizeAttributes":[
        "FirstName=profile/givenName",
         "LastName=profile/familyName",
         "Email=profile/email"
    ],
    "addGroupMemberships": false,
        "defaultGroups": [
        "app-aem-no-access"
    ],
    "groupMembershipAttribute": "Groups",  
     "nameIdFormat": "urn:oasis:names:tc:SAML:1.1:nameid-format:emailAddress"
}

