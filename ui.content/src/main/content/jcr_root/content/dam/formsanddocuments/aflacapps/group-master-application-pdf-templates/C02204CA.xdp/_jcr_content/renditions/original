            <variables>
               <script contentType="application/x-javascript" name="scriptDE">
function hideseg(){
if(DE1.GroupDental.GA.TextField25.rawValue=="Group Dental"){
DE1.GroupDental.GA.TextField25.rawValue=DE1.GroupDental.GA.TextField25.rawValue.toUpperCase();
}
else{
DE1.presence="hidden";
}
}

function hide(){
if(DE1.GroupDentalPremiumContribution.GroupPolicy.CheckBox15.rawValue=="No") {
DE1.GroupDentalPremiumContribution.TextField19.presence="hidden";
DE1.GroupDentalPremiumContribution.TextField20.presence="hidden";
DE1.GroupDentalPremiumContribution.GroupPolicy.CheckBox14.presence="hidden";
}
else{
DE1.GroupDentalPremiumContribution.GroupPolicy.CheckBox15.presence="hidden";
}
}

function hideCB(){
tl=xfa.resolveNodes("DE1.GroupDentalOptionalFeatures.Table1.Row1.productCoverages.productCoverage[*]").length;
for(i=0;i&lt;tl;i++){
if(xfa.resolveNode("DE1.GroupDentalOptionalFeatures.Table1.Row1.productCoverages.productCoverage["+(i)+"].coveragelevel.productCoverageLevel").rawValue==null){
xfa.resolveNode("DE1.GroupDentalOptionalFeatures.Table1.Row1.productCoverages.productCoverage["+(i)+"].coveragelevel.CheckBox21").presence="hidden";
}
}
}

function hideothers(){
if(DE1.GroupDentalEligibleEmployees.Eligible3.TextField16.rawValue==null){
DE1.GroupDentalEligibleEmployees.Eligible3.presence="hidden";
}
}

function hideexcept(){
if(DE1.GroupDentalEligibleEmployees.TextField15.rawValue==null){
DE1.GroupDentalEligibleEmployees.TextField15.presence="hidden";
}
}

function accidentreason(){
if(DE1.GroupDental.ApplicationReason.CheckBox1.rawValue=="New Application"){
DE1.GroupDental.ApplicationReason.CheckBox3.presence="hidden";
DE1.GroupDental.ApplicationReason.TextField13.presence="hidden";
DE1.GroupDental.Other.presence="hidden";
}
else if(DE1.GroupDental.ApplicationReason.CheckBox3.rawValue=="Change to Existing Aflac Policy"){
DE1.GroupDental.ApplicationReason.CheckBox1.presence="hidden";
DE1.GroupDental.Other.presence="hidden";
}
else if(DE1.GroupDental.Other.CheckBox4.rawValue=="Other"){
DE1.GroupDental.ApplicationReason.presence="hidden";
}
}

function age(){
if(DE1.GroupDentalEligibleEmployees.Eligible2.TextField2.rawValue=="Between Age"){
xfa.resolveNode("DE1.GroupDentalEligibleEmployees.Eligible2.TextField3").rawValue=xfa.resolveNode("DE1.GroupDentalEligibleEmployees.Eligible2.TextField3").rawValue+" and ";
xfa.resolveNode("DE1.GroupDentalEligibleEmployees.Eligible2.TextField5").rawValue="";
}
}

function hidespouse(){
if(DE1.GroupDentalEligibleEmployees.Eligible2.TextField21.rawValue=="Yes"){
DE1.GroupDentalEligibleEmployees.Eligible2.TextField21.presence="hidden"
}
else{
DE1.GroupDentalEligibleEmployees.Eligible2.presence="hidden"
}
}

function ageemp(){
if(DE1.GroupDentalEligibleEmployees.Eligible1.TextField2.rawValue=="Between Age"){
xfa.resolveNode("DE1.GroupDentalEligibleEmployees.Eligible1.TextField3").rawValue=xfa.resolveNode("DE1.GroupDentalEligibleEmployees.Eligible1.TextField3").rawValue+" and ";
xfa.resolveNode("DE1.GroupDentalEligibleEmployees.Eligible1.TextField5").rawValue="";
}
}

function hideplan(){
if(DE1.GroupDental.GA.CheckBox17.rawValue==1){
DE1.GroupDental.GA.CheckBox18.presence="hidden";
DE1.GroupDental.GA.CheckBox19.presence="hidden";
DE1.GroupDental.GA.CheckBox20.presence="hidden";
}
else if(DE1.GroupDental.GA.CheckBox17.rawValue==2){
DE1.GroupDental.GA.CheckBox17.presence="hidden";
DE1.GroupDental.GA.CheckBox19.presence="hidden";
DE1.GroupDental.GA.CheckBox20.presence="hidden";
}
else if(DE1.GroupDental.GA.CheckBox17.rawValue==3){
DE1.GroupDental.GA.CheckBox18.presence="hidden";
DE1.GroupDental.GA.CheckBox17.presence="hidden";
DE1.GroupDental.GA.CheckBox20.presence="hidden";
}
else if(DE1.GroupDental.GA.CheckBox17.rawValue==4){
DE1.GroupDental.GA.CheckBox18.presence="hidden";
DE1.GroupDental.GA.CheckBox19.presence="hidden";
DE1.GroupDental.GA.CheckBox17.presence="hidden";
}
}
</script>
