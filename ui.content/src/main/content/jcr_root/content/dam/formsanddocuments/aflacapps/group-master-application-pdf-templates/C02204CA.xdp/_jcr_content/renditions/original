         <subform layout="tb" name="CI1" w="576pt">
            <variables>
               <script contentType="application/x-javascript" name="scriptCI">
function hideseg(){
if(CI1.CriticalIllness.CI.TextField21.rawValue=="Group Critical Illness"){
CI1.CriticalIllness.CI.TextField21.rawValue=CI1.CriticalIllness.CI.TextField21.rawValue.toUpperCase();
}
else{
CI1.presence="hidden";
}
}

function hide(){
if(CI1.CriticalIllnessPremiumContribution.GroupPolicy.CheckBox15.rawValue=="No") {
CI1.CriticalIllnessPremiumContribution.TextField19.presence="hidden";
CI1.CriticalIllnessPremiumContribution.TextField20.presence="hidden";
CI1.CriticalIllnessPremiumContribution.GroupPolicy.CheckBox14.presence="hidden";
}
else{
CI1.CriticalIllnessPremiumContribution.GroupPolicy.CheckBox15.presence="hidden";
}
}

function hideCB(){
tl=xfa.resolveNodes("CI1.CriticalIllnessOptionalFeatures.Table1.Row1.productCoverages.productCoverage[*]").length;
for(i=0;i&lt;tl;i++){
if(xfa.resolveNode("CI1.CriticalIllnessOptionalFeatures.Table1.Row1.productCoverages.productCoverage["+(i)+"].coveragelevel.productCoverageLevel").rawValue==null){
xfa.resolveNode("CI1.CriticalIllnessOptionalFeatures.Table1.Row1.productCoverages.productCoverage["+(i)+"].coveragelevel.CheckBox20").presence="hidden";
}
}
}

function hideothers(){
if(CI1.CriticalIllnessEligibleEmployees.Eligible3.TextField16.rawValue==null){
CI1.CriticalIllnessEligibleEmployees.Eligible3.presence="hidden";
}
}

function hideexcept(){
if(CI1.CriticalIllnessEligibleEmployees.TextField15.rawValue==null){
CI1.CriticalIllnessEligibleEmployees.TextField15.presence="hidden";
}
}

function accidentreason(){
if(CI1.CriticalIllness.ApplicationReason.CheckBox1.rawValue=="New Application"){
CI1.CriticalIllness.ApplicationReason.CheckBox3.presence="hidden";
CI1.CriticalIllness.ApplicationReason.TextField13.presence="hidden";
CI1.CriticalIllness.Other.presence="hidden";
}
else if(CI1.CriticalIllness.ApplicationReason.CheckBox3.rawValue=="Change to Existing Aflac Policy"){
CI1.CriticalIllness.ApplicationReason.CheckBox1.presence="hidden";
CI1.CriticalIllness.Other.presence="hidden";
}
else if(CI1.CriticalIllness.Other.CheckBox4.rawValue=="Other"){
CI1.CriticalIllness.ApplicationReason.presence="hidden";
}
}

function age(){
if(CI1.CriticalIllnessEligibleEmployees.Eligible2.TextField2.rawValue=="Between Age"){
xfa.resolveNode("CI1.CriticalIllnessEligibleEmployees.Eligible2.TextField3").rawValue=xfa.resolveNode("CI1.CriticalIllnessEligibleEmployees.Eligible2.TextField3").rawValue+" and ";
xfa.resolveNode("CI1.CriticalIllnessEligibleEmployees.Eligible2.TextField5").rawValue="";
}
}

function hidespouse(){
if(CI1.CriticalIllnessEligibleEmployees.Eligible2.TextField21.rawValue=="Yes"){
CI1.CriticalIllnessEligibleEmployees.Eligible2.TextField21.presence="hidden"
}
else{
CI1.CriticalIllnessEligibleEmployees.Eligible2.presence="hidden"
}
}

function ageemp(){
if(CI1.CriticalIllnessEligibleEmployees.Eligible1.TextField2.rawValue=="Between Age"){
xfa.resolveNode("CI1.CriticalIllnessEligibleEmployees.Eligible1.TextField3").rawValue=xfa.resolveNode("CI1.CriticalIllnessEligibleEmployees.Eligible1.TextField3").rawValue+" and ";
xfa.resolveNode("CI1.CriticalIllnessEligibleEmployees.Eligible1.TextField5").rawValue="";
}
}
