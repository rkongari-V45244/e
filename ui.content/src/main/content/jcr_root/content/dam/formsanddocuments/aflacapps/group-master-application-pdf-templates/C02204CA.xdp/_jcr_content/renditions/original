            <variables>
               <script contentType="application/x-javascript" name="scriptWL">
function hideseg(){
if(WL1.GroupWholeLife.GA.TextField30.rawValue=="Group Whole Life"){
WL1.GroupWholeLife.GA.TextField30.rawValue=WL1.GroupWholeLife.GA.TextField30.rawValue.toUpperCase();
}
else{
WL1.presence="hidden";
}
}

function hide(){
if(WL1.GroupWholeLifePremiumContribution.GroupPolicy.CheckBox15.rawValue=="No") {

WL1.TextField19.presence="hidden";
WL1.TextField20.presence="hidden";
WL1.GroupWholeLifePremiumContribution.GroupPolicy.CheckBox14.presence="hidden";
}
else{
WL1.GroupWholeLifePremiumContribution.GroupPolicy.CheckBox15.presence="hidden";
}
}

function hideCB(){
tl=xfa.resolveNodes("WL1.GroupWholeLifeOptionalFeatures.Table1.Row1.productCoverages.productCoverage[*]").length;
for(i=0;i&lt;tl;i++){
if(xfa.resolveNode("WL1.GroupWholeLifeOptionalFeatures.Table1.Row1.productCoverages.productCoverage["+(i)+"].coveragelevel.productCoverageLevel").rawValue==null){
xfa.resolveNode("WL1.GroupWholeLifeOptionalFeatures.Table1.Row1.productCoverages.productCoverage["+(i)+"].coveragelevel.CheckBox25").presence="hidden";
}
}
}

function hideothers(){
if(WL1.GroupWholeLifeEligibleEmployees.Eligible3.TextField16.rawValue==null){
WL1.GroupWholeLifeEligibleEmployees.Eligible3.presence="hidden";
}
}

function hideexcept(){
if(WL1.GroupWholeLifeEligibleEmployees.TextField15.rawValue==null){
WL1.GroupWholeLifeEligibleEmployees.TextField15.presence="hidden";
}
}

function accidentreason(){
if(WL1.GroupWholeLife.ApplicationReason.CheckBox1.rawValue=="New Application"){
WL1.GroupWholeLife.ApplicationReason.CheckBox3.presence="hidden";
WL1.GroupWholeLife.ApplicationReason.TextField13.presence="hidden";
WL1.GroupWholeLife.Other.presence="hidden";
}
else if(WL1.GroupWholeLife.ApplicationReason.CheckBox3.rawValue=="Change to Existing Aflac Policy"){
WL1.GroupWholeLife.ApplicationReason.CheckBox1.presence="hidden";
WL1.GroupWholeLife.Other.presence="hidden";
}
else if(WL1.GroupWholeLife.Other.CheckBox4.rawValue=="Other"){
WL1.GroupWholeLife.ApplicationReason.presence="hidden";
}
}

function ageemp(){
if(WL1.GroupWholeLifeEligibleEmployees.Eligible1.TextField2.rawValue=="Between Age"){
xfa.resolveNode("WL1.GroupWholeLifeEligibleEmployees.Eligible1.TextField3").rawValue=xfa.resolveNode("WL1.GroupWholeLifeEligibleEmployees.Eligible1.TextField3").rawValue+" and ";
xfa.resolveNode("WL1.GroupWholeLifeEligibleEmployees.Eligible1.TextField5").rawValue="";
}
}

function hidespouse(){
if(WL1.GroupWholeLifeEligibleEmployees.Eligible2.TextField21.rawValue=="Yes"){
WL1.GroupWholeLifeEligibleEmployees.Eligible2.TextField21.presence="hidden"
}
else{
WL1.GroupWholeLifeEligibleEmployees.Eligible2.presence="hidden"
}
}

function age(){
if(WL1.GroupWholeLifeEligibleEmployees.Eligible2.TextField2.rawValue=="Between Age"){
xfa.resolveNode("WL1.GroupWholeLifeEligibleEmployees.Eligible2.TextField3").rawValue=xfa.resolveNode("WL1.GroupWholeLifeEligibleEmployees.Eligible2.TextField3").rawValue+" and ";
xfa.resolveNode("WL1.GroupWholeLifeEligibleEmployees.Eligible2.TextField5").rawValue="";
}
}

</script>
