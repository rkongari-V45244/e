               <script contentType="application/x-javascript" name="scripts">
function hideseg(){
if(AC1.GroupAccident.GA.TextField23.rawValue=="Group Accident"){
AC1.GroupAccident.GA.TextField23.rawValue=AC1.GroupAccident.GA.TextField23.rawValue.toUpperCase();
}
else{
AC1.presence="hidden";
}
}

function hide(){
if(AC1.GroupAccidentPremiumContribution.GroupPolicy.CheckBox15.rawValue=="No") {
AC1.GroupAccidentPremiumContribution.TextField19.presence="hidden";
AC1.GroupAccidentPremiumContribution.TextField20.presence="hidden";
AC1.GroupAccidentPremiumContribution.GroupPolicy.CheckBox14.presence="hidden";
}
else{
AC1.GroupAccidentPremiumContribution.GroupPolicy.CheckBox15.presence="hidden";
}

}

function hideCB(){
tl=xfa.resolveNodes("AC1.GroupAccidentOptionalFeatures.Table1.Row1.productCoverages.productCoverage[*]").length;
for(i=0;i&lt;tl;i++){
if(xfa.resolveNode("AC1.GroupAccidentOptionalFeatures.Table1.Row1.productCoverages.productCoverage["+(i)+"].coveragelevel.productCoverageLevel").rawValue==null){
xfa.resolveNode("AC1.GroupAccidentOptionalFeatures.Table1.Row1.productCoverages.productCoverage["+(i)+"].coveragelevel.CheckBox20").presence="hidden";
}
}
}

function hideothers(){
if(AC1.GroupAccidentEligibleEmployees.Eligible3.TextField16.rawValue==null){
AC1.GroupAccidentEligibleEmployees.Eligible3.presence="hidden";
}
}

function hideexcept(){
if(AC1.GroupAccidentEligibleEmployees.TextField15.rawValue==null){
AC1.GroupAccidentEligibleEmployees.TextField15.presence="hidden";
}
}

function accidentreason(){
if(AC1.GroupAccident.ApplicationReason.CheckBox1.rawValue=="New Application"){
AC1.GroupAccident.ApplicationReason.CheckBox3.presence="hidden";
AC1.GroupAccident.ApplicationReason.TextField13.presence="hidden";
AC1.GroupAccident.Other.presence="hidden";
}
else if(AC1.GroupAccident.ApplicationReason.CheckBox3.rawValue=="Change to Existing Aflac Policy"){
AC1.GroupAccident.ApplicationReason.CheckBox1.presence="hidden";
AC1.GroupAccident.Other.presence="hidden";
}
else if(AC1.GroupAccident.Other.CheckBox4.rawValue=="Other"){
AC1.GroupAccident.ApplicationReason.presence="hidden";
}
}

function age(){
if(AC1.GroupAccidentEligibleEmployees.Eligible2.TextField2.rawValue=="Between Age"){
xfa.resolveNode("AC1.GroupAccidentEligibleEmployees.Eligible2.TextField3").rawValue=xfa.resolveNode("AC1.GroupAccidentEligibleEmployees.Eligible2.TextField3").rawValue+" and ";
xfa.resolveNode("AC1.GroupAccidentEligibleEmployees.Eligible2.TextField5").rawValue="";
}
}

function hidespouse(){
if(AC1.GroupAccidentEligibleEmployees.Eligible2.TextField21.rawValue=="Yes"){
AC1.GroupAccidentEligibleEmployees.Eligible2.TextField21.presence="hidden"
}
else{
AC1.GroupAccidentEligibleEmployees.Eligible2.presence="hidden"
}
}

function ageemp(){
if(AC1.GroupAccidentEligibleEmployees.Eligible1.TextField2.rawValue=="Between Age"){
xfa.resolveNode("AC1.GroupAccidentEligibleEmployees.Eligible1.TextField3").rawValue=xfa.resolveNode("AC1.GroupAccidentEligibleEmployees.Eligible1.TextField3").rawValue+" and ";
xfa.resolveNode("AC1.GroupAccidentEligibleEmployees.Eligible1.TextField5").rawValue="";
}
}

function hideplan(){
if(AC1.GroupAccidentOptionalFeatures.Plan.CheckBox17.rawValue==1){
AC1.GroupAccidentOptionalFeatures.Plan.CheckBox18.presence="hidden";
AC1.GroupAccidentOptionalFeatures.Plan.CheckBox19.presence="hidden";
}
else if(AC1.GroupAccidentOptionalFeatures.Plan.CheckBox18.rawValue==2){
AC1.GroupAccidentOptionalFeatures.Plan.CheckBox17.presence="hidden";
AC1.GroupAccidentOptionalFeatures.Plan.CheckBox19.presence="hidden";
}
else if(AC1.GroupAccidentOptionalFeatures.Plan.CheckBox19.rawValue==3){
AC1.GroupAccidentOptionalFeatures.Plan.CheckBox18.presence="hidden";
AC1.GroupAccidentOptionalFeatures.Plan.CheckBox17.presence="hidden";
}
}
</script>
