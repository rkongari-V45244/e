function grammer(){
if(xfa.resolveNode("Req1.Req1_Structure.General1.type[1]").rawValue=="Employee" || xfa.resolveNode("Req1.Req1_Structure.General1.type[1]").rawValue=="Associate"){
Req1.Req1_Structure.General1.grammer1.rawValue = "An";
Req1.Req1_Structure.General1.grammer2.rawValue = "An";
Req1.Req1_Structure.General1.grammer3.rawValue = "An";
}
else if(xfa.resolveNode("Req1.Req1_Structure.General1.type[1]").rawValue=="Member"){
Req1.Req1_Structure.General1.grammer1.rawValue = "A";
Req1.Req1_Structure.General1.grammer2.rawValue = "A";
Req1.Req1_Structure.General1.grammer3.rawValue = "A";
}
}
