                  <empType/>
                  <empAgeRange>
                     <range/>
                     <age dd:minOccur="0" dd:nullType="exclude"/>
                     <minAge dd:minOccur="0" dd:nullType="exclude"/>
                     <maxAge dd:minOccur="0" dd:nullType="exclude"/>
                  </empAgeRange>
                  <exception dd:minOccur="0" dd:nullType="exclude"/>
                  <spouse>
                     <isEligible/>
                     <empAgeRange dd:minOccur="0">
                        <range/>
                        <age dd:minOccur="0" dd:nullType="exclude"/>
                        <minAge dd:minOccur="0" dd:nullType="exclude"/>
                        <maxAge dd:minOccur="0" dd:nullType="exclude"/>
                     </empAgeRange>
                  </spouse>
