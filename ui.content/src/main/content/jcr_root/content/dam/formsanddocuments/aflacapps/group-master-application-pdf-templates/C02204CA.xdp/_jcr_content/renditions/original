            <variables>
               <script contentType="application/x-javascript" name="scriptDI">
function hideseg(){
if(DI1.GroupDisabilityIncome.GA.TextField26.rawValue=="Group Disability Income"){
DI1.GroupDisabilityIncome.GA.TextField26.rawValue=DI1.GroupDisabilityIncome.GA.TextField26.rawValue.toUpperCase();
}
else{
DI1.presence="hidden";
}
}

function hide(){
if(DI1.GroupDisabilityIncomePremiumContribution.GroupPolicy.CheckBox15.rawValue=="No") {
DI1.GroupDisabilityIncomePremiumContribution.TextField19.presence="hidden";
DI1.GroupDisabilityIncomePremiumContribution.TextField20.presence="hidden";
DI1.GroupDisabilityIncomePremiumContribution.GroupPolicy.CheckBox14.presence="hidden";
}
else{
DI1.GroupDisabilityIncomePremiumContribution.GroupPolicy.CheckBox15.presence="hidden";
}
}

function hideCB(){
tl=xfa.resolveNodes("DI1.GroupDisabilityIncomeOptionalFeatures.Table1.Row1.productCoverages.productCoverage[*]").length;
for(i=0;i&lt;tl;i++){
if(xfa.resolveNode("DI1.GroupDisabilityIncomeOptionalFeatures.Table1.Row1.productCoverages.productCoverage["+(i)+"].coveragelevel.productCoverageLevel").rawValue==null){
xfa.resolveNode("DI1.GroupDisabilityIncomeOptionalFeatures.Table1.Row1.productCoverages.productCoverage["+(i)+"].coveragelevel.CheckBox23").presence="hidden";
}
}
}

function hideothers(){
if(DI1.GroupDisabilityIncomeEligibleEmployees.Eligible3.TextField16.rawValue==null){
DI1.GroupDisabilityIncomeEligibleEmployees.Eligible3.presence="hidden";
}
}

function hideexcept(){
if(DI1.GroupDisabilityIncomeEligibleEmployees.TextField15.rawValue==null){
DI1.GroupDisabilityIncomeEligibleEmployees.TextField15.presence="hidden";
}
}

function accidentreason(){
if(DI1.GroupDisabilityIncome.ApplicationReason.CheckBox1.rawValue=="New Application"){
DI1.GroupDisabilityIncome.ApplicationReason.CheckBox3.presence="hidden";
DI1.GroupDisabilityIncome.ApplicationReason.TextField13.presence="hidden";
DI1.GroupDisabilityIncome.Other.presence="hidden";
}
else if(DI1.GroupDisabilityIncome.ApplicationReason.CheckBox3.rawValue=="Change to Existing Aflac Policy"){
DI1.GroupDisabilityIncome.ApplicationReason.CheckBox1.presence="hidden";
DI1.GroupDisabilityIncome.Other.presence="hidden";
}
else if(DI1.GroupDisabilityIncome.Other.CheckBox4.rawValue=="Other"){
DI1.GroupDisabilityIncome.ApplicationReason.presence="hidden";
}
}

function ageemp(){
if(DI1.GroupDisabilityIncomeEligibleEmployees.Eligible1.TextField2.rawValue=="Between Age"){
xfa.resolveNode("DI1.GroupDisabilityIncomeEligibleEmployees.Eligible1.TextField3").rawValue=xfa.resolveNode("DI1.GroupDisabilityIncomeEligibleEmployees.Eligible1.TextField3").rawValue+" and ";
xfa.resolveNode("DI1.GroupDisabilityIncomeEligibleEmployees.Eligible1.TextField5").rawValue="";
}
}
</script>
